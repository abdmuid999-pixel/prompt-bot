{
  "name": "prompt-bot",
  "version": "0.1.0",
  "main": "electron-main.js",
  "scripts": {
    "dev": "concurrently \"node backend/server.js\" \"cd renderer && npm run dev\" \"electron .\"",
    "start": "electron .",
    "build:renderer": "cd renderer && npm run build",
    "package": "npm run build:renderer && electron-builder --win --x64"
  },
  "devDependencies": {
    "electron": "^26.0.0",
    "electron-builder": "^24.0.0",
    "concurrently": "^8.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.4.0",
    "lowdb": "^5.0.0",
    "multer": "^1.4.5-lts.1",
    "simple-encryptor": "^1.0.0",
    "google-generativeai": "^0.0.2",
    "openai": "^4.0.0",
    "dotenv": "^16.0.0"
  }
}